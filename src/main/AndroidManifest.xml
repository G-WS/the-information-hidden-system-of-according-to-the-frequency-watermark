<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.security">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/you_define_me"
        android:label="@string/app_name"
        android:roundIcon="@drawable/you_define_me"
        android:supportsRtl="true"
        android:theme="@style/Theme.Security"
        android:usesCleartextTraffic="true">
        <activity android:name=".ExtraPlus"></activity>
        <activity android:name=".ExtraActivity" />
        <activity android:name=".LSBActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".TextActivitiy" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>